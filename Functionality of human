<DOCTYPE html>
    <html>
        <head>
            <title>Classes and Objects in js</title>
        </head>
        <script>

            class human{
                sleep(){
                    document.write("Human is sleeping","<br>")
                }
                eat(){
                    document.write("Human is eating","<br>")
                }
                learn(){
                    document.write("Human is learning","<br>")
                }
          }
          var akash = new human()
          var farhan = new human()
          var iraj = new human()
          var anna = new human()
          </script>
    </html>
