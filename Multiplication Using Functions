<!DOCTYPE html>
<html>

<head>
</head>

<body>
    <button onclick="multiply(4)">Click Me!</button>
    <p id="result"></p>
    <p id="result1"></p>

    <script>
        function multiply(a, b) {
            if (b === undefined) {
                alert("The value of b is undefined");
                b = 5;
            }
            var result = a * b;
            document.getElementById("result").innerHTML = result;
            var result = a + b;
            document.getElementById("result1").innerHTML = result; 
        }
        
    </script>
</body>

</html>
